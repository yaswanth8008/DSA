
# https://www.geeksforgeeks.org/problems/count-nodes-of-linked-list/1?page=1&category=Linked%20List&sortBy=submissions

class LinkedList:
    def __init__(self):
        self.head=None
        self.tail=None
        
class Solution:
    # Function to count nodes of a linked list.
    def getCount(self, head):
        # code here
        cnt = 0 
        temp = head 
        while temp != None:
            cnt += 1
            temp = temp.next 
        return cnt 
    
